  pythonProcess.on("close", (code) => {
    console.log(`ğŸ Python process completed with code: ${code}`);
    console.log(`ğŸ“¤ Python stdout: ${stdout.trim()}`);
    console.log(`ğŸ“¥ Python stderr: ${stderr.trim()}`);
    
    if (code !== 0) {
      console.error("âŒ Detection generation failed:", stderr);
      res.status(500).json({
        error: "Failed to generate detections",
        details: stderr.trim() || stdout.trim(),
      });
      return;
    }

    console.log(`ğŸ” Checking detection count for session ${sessionId}...`);
    db.get(
      "SELECT COUNT(*) as count FROM tracked_objects WHERE session_id = ?",
      [sessionId],
      (countErr, row) => {
        if (countErr) {
          console.error("âŒ Database error after generation:", countErr);
          res.status(500).json({
            error: "Detections generated but counting results failed",
          });
          return;
        }

        const detectionCount = row?.count ?? 0;
        console.log(`ğŸ“Š Detection count in database: ${detectionCount}`);
        
        res.json({
          message: "Detections generated successfully",
          detections: detectionCount,
          output: stdout.trim(),
        });
        
        console.log(`âœ… Regenerate response sent: ${detectionCount} detections`);
      }
    );
  });
